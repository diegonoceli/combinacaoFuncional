<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_about"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.usuario.myapplication.About"
    tools:showIn="@layout/activity_about">

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/facebookicon"
        android:id="@+id/fbdiego"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/fbfaeda"
        android:layout_marginEnd="14dp"
        app:layout_constraintRight_toRightOf="@+id/Desenvoldero1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/Desenvoldero1"
        app:layout_constraintTop_toTopOf="@+id/fbfaeda"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="14dp" />

    <ImageView
        android:layout_width="49dp"
        app:srcCompat="@drawable/facebookicon"
        android:id="@+id/fbdenis"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/fbfaeda"
        app:layout_constraintRight_toRightOf="@+id/denis"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/denis"
        app:layout_constraintTop_toTopOf="@+id/fbfaeda"
        android:layout_height="44dp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/diego"
        android:layout_width="87dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/diego"
        app:layout_constraintRight_toLeftOf="@+id/Desenvolvedor2"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="29dp"
        app:layout_constraintBottom_toBottomOf="@+id/faedaimg"
        android:layout_marginEnd="31dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/faedaimg"
        android:layout_marginLeft="29dp"
        android:layout_marginRight="31dp" />

    <TextView
        android:text="Aplicação criada para facilitar o aprendizado de combinações de numeros."
        android:layout_width="0dp"
        android:layout_height="99dp"
        android:id="@+id/descricao"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="38dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="63dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="38dp"
        android:layout_marginRight="38dp" />

    <TextView
        android:text="Diego Noceli"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/diegotxt"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        app:layout_constraintRight_toLeftOf="@+id/faeda"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="21dp"
        app:layout_constraintBottom_toBottomOf="@+id/faeda"
        android:layout_marginEnd="26dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/faeda"
        android:layout_marginLeft="21dp"
        android:layout_marginRight="26dp" />

    <TextView
        android:text="Leonardo Faeda"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:id="@+id/faeda"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Desenvoldero1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="11dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="Desenvolvedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvoldero1"
        android:textSize="13sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="5dp"
        app:layout_constraintRight_toRightOf="@+id/diego"
        android:layout_marginBottom="229dp"
        android:layout_marginRight="5dp" />
    <TextView
        android:text="Orientador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvolvedor"
        android:textSize="13sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/Desenvolvedor2"
        android:layout_marginEnd="7dp"
        app:layout_constraintRight_toRightOf="@+id/denis"
        app:layout_constraintTop_toTopOf="@+id/Desenvolvedor2"
        android:layout_marginRight="7dp" />
    <TextView
        android:text="Desenvolvedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvolvedor2"
        android:textSize="13sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/faeda"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/faeda"
        android:layout_marginLeft="10dp" />

    <TextView
        android:text="Dênis Vargas"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/denistxt"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/Desenvolvedor"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="5dp"
        app:layout_constraintTop_toTopOf="@+id/faeda"
        app:layout_constraintLeft_toRightOf="@+id/faeda"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="16dp" />

    <ImageView
        android:layout_width="49dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/facebookicon"
        android:id="@+id/fbfaeda"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="65dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/denis"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/denis"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/faedaimg"
        android:layout_marginEnd="7dp"
        app:layout_constraintRight_toRightOf="@+id/denistxt"
        app:layout_constraintTop_toBottomOf="@+id/Desenvolvedor"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/denistxt"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="7dp" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/faedaimg"
        android:layout_width="79dp"
        android:layout_height="0dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/diego"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/fbfaeda"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="26dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Desenvoldero1" />

</android.support.constraint.ConstraintLayout>
