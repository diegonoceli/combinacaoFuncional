<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_about"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.usuario.myapplication.About"
    tools:showIn="@layout/activity_about"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/fbdiego"
        android:layout_width="49dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/facebookicon"
        tools:layout_editor_absoluteY="409dp"
        app:layout_constraintRight_toLeftOf="@+id/fbfaeda"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:layout_width="49dp"
        app:srcCompat="@drawable/facebookicon"
        android:id="@+id/fbdenis"
        android:layout_height="44dp"
        tools:layout_editor_absoluteY="409dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/fbfaeda" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/diego"
        android:layout_width="87dp"
        android:layout_height="110dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/diego"
        tools:layout_editor_absoluteY="290dp"
        app:layout_constraintRight_toLeftOf="@+id/faedaimg"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="Aplicação criada para facilitar o aprendizado de combinações de numeros."
        android:layout_width="0dp"
        android:layout_height="99dp"
        android:id="@+id/descricao"
        android:textSize="24sp"
        tools:layout_editor_absoluteY="44dp"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:text="Leonardo Faeda"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:id="@+id/faeda"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        tools:layout_editor_absoluteY="212dp"
        app:layout_constraintRight_toLeftOf="@+id/denistxt"
        app:layout_constraintLeft_toRightOf="@+id/diegotxt" />

    <TextView
        android:text="Desenvolvedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvoldero1"
        android:textSize="13sp"
        tools:layout_editor_absoluteY="263dp"
        app:layout_constraintRight_toLeftOf="@+id/Desenvolvedor2"
        app:layout_constraintLeft_toLeftOf="parent" />
    <TextView
        android:text="Orientador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvolvedor"
        android:textSize="13sp"
        tools:layout_editor_absoluteY="263dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Desenvolvedor2" />
    <TextView
        android:text="Desenvolvedor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Desenvolvedor2"
        android:textSize="13sp"
        tools:layout_editor_absoluteY="263dp"
        app:layout_constraintRight_toLeftOf="@+id/Desenvolvedor"
        app:layout_constraintLeft_toRightOf="@+id/Desenvoldero1" />

    <TextView
        android:id="@+id/denistxt"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:text="Dênis Vargas"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        tools:layout_editor_absoluteY="212dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/faeda" />

    <ImageView
        android:layout_width="49dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/facebookicon"
        android:id="@+id/fbfaeda"
        tools:layout_editor_absoluteY="409dp"
        app:layout_constraintRight_toLeftOf="@+id/fbdenis"
        app:layout_constraintLeft_toRightOf="@+id/fbdiego" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/denis"
        android:layout_width="79dp"
        android:layout_height="110dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/denis"
        tools:layout_editor_absoluteY="290dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/faedaimg" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/faedaimg"
        android:layout_width="79dp"
        android:layout_height="110dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/diego"
        tools:layout_editor_absoluteY="290dp"
        app:layout_constraintRight_toLeftOf="@+id/denis"
        app:layout_constraintLeft_toRightOf="@+id/diego" />

    <TextView
        android:id="@+id/diegotxt"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:text="Diego Noceli"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="18sp"
        tools:layout_editor_absoluteY="212dp"
        app:layout_constraintRight_toLeftOf="@+id/faeda"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>
